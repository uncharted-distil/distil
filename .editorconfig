# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.{vue,ts,ejs,js,css,html,md}]
indent_style = space
indent_size = 2

[package.json,.travis.yml]
indent_size = 2
indent_style = space

[*.csv]
end_of_line = crlf
